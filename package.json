{
  "dependencies": {
    "lodash": "^3.10.0",
    "react": "^0.13.3",
    "superagent": "^1.2.0"
  },
  "scripts": {
    "serve": "http-server build",
    "livereload": "livereload build -i 100",
    "watch": "watchify src/index.js -t babelify --outfile build/bundle.js -v --debug",
    "watch-and-serve": "npm run watch & npm run livereload & npm run serve",
    "build": "browserify src/index.js -t babelify | uglifyjs --compress --mangle > build/bundle.js"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^10.2.6",
    "uglify-js": "^2.4.23",
    "watchify": "^3.2.3"
  }
}
